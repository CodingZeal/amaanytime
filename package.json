{
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "^18.12.1",
    "yarn": "^3.3.1"
  },
  "prisma": {
    "seed": "yarn rw exec seed",
    "schema": "api/db/schema.prisma"
  },
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "start": "node index.js",
    "serve": "yarn rw serve",
    "build": "yarn rw build",
    "lint": "yarn eslint && yarn pretty",
    "eslint": "npx eslint .",
    "pretty": "npx prettier --check .",
    "dev": "./scripts/dev.sh",
    "playwright": "npx playwright test -c web/playwright.config.ts --debug",
    "test:e2e": "./scripts/e2e.ts",
    "test": "yarn rw test",
    "schema:format": "npx prisma format",
    "db:setup": "yarn rw prisma migrate dev && yarn rw prisma db seed"
  },
  "dependencies": {
    "@redwoodjs/core": "^3.8.0"
  }
}
